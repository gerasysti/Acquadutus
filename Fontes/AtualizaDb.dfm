        Align = alTop
        Lines.Strings = (
          '<DATA_ATUALIZACAO>'
          '26/10/2011'
          '<TEXTO_ATUALIZACAO>'
          'ALTER TABLE CTRL_PROCES_SUB_UND_ORCAM'
          
            'ADD DESCR_SUB_UNID_ORCAM COMPUTED BY ((select descricao from sub' +
            '_unid_orcament'
          ' where id = CTRL_PROCES_SUB_UND_ORCAM.id_sub_unid_orcam))'
          '<MENSAGEM>'
          
            'Foi criado o campo "DESCR_SUB_UNID_ORCAM" na tabela "CTRL_PROCES' +
            '_SUB_UND_ORCAM"'
          '<TEXTO_ATUALIZACAO>'
          'ALTER TABLE CTRL_PROCES_SUB_UND_ORCAM'
          
            'ADD ID_UNID_GESTORA COMPUTED BY ((select id_unid_gestora from su' +
            'b_unid_orcament'
          ' where id = CTRL_PROCES_SUB_UND_ORCAM.id_sub_unid_orcam))'
          '<MENSAGEM>'
          
            'Foi criado o campo "ID_UNID_GESTORA" na tabela "CTRL_PROCES_SUB_' +
            'UND_ORCAM"'
          '<DATA_ATUALIZACAO>'
          '17/11/2011'
          '<TEXTO_ATUALIZACAO>'
          'create or alter procedure SP_REL_SERVID_SUO ('
          '    PE_UNID_GESTORA "INTEGER",'
          '    PE_SUB_UNID_ORCAMS "VARCHAR(120)",'
          '    PE_UNID_LOTACOES "VARCHAR(120)",'
          '    PE_CARGOS "VARCHAR(120)",'
          '    PE_EST_FUNCIONAIS "VARCHAR(80)",'
          '    PE_VINCULO "CHAR(1)",'
          '    PE_SITUAC "CHAR(1)",'
          '    PE_DT_ADMIN_INI "DATE",'
          '    PE_DT_ADMIN_FIM "DATE")'
          'returns ('
          '    R_ID_SUB_UNID_ORCAMENT "INTEGER",'
          '    R_DESCR_SUB_UNID_ORCAMENT "VARCHAR(60)",'
          '    R_ID_SERVIDOR "INTEGER",'
          '    R_ID_SERVIDOR_FTDO "CHAR(9)",'
          '    R_ID_PESSOA_FISICA "INTEGER",'
          '    R_NOME_SERVIDOR "VARCHAR(60)",'
          '    R_DT_NASCIMENTO "DATE",'
          '    R_ID_CARGO_ORIGEM "INTEGER",'
          '    R_DESCR_CARGO_ORIGEM "VARCHAR(60)",'
          '    R_ID_CARGO_ATUAL "INTEGER",'
          '    R_DESCR_CARGO_ATUAL "VARCHAR(60)",'
          '    R_DT_ADMISSAO "DATE",'
          '    R_DT_READMISSAO "DATE",'
          '    R_DT_ADMISSAO2 "DATE",'
          '    R_REF_SALARIO "INTEGER",'
          '    R_VENCTO_BASE "NUMERIC(15,4)",'
          '    R_TIPO_SAL "CHAR(1)",'
          '    R_DESCR_TIPO_SAL "VARCHAR(10)",'
          '    R_ID_UNID_LOTACAO "INTEGER",'
          '    R_DESCR_UNID_LOTACAO "VARCHAR(60)",'
          '    R_ID_EST_FUNCIONAL "INTEGER",'
          '    R_DESCR_EST_FUNCIONAL "VARCHAR(60)",'
          '    R_DESCR_SITUAC_TCM "VARCHAR(60)",'
          '    R_EFETIVO "CHAR(1)",'
          '    R_ID_SITUACAO_TCM "INTEGER",'
          '    R_DESCR_EFETIVO "CHAR(3)",'
          '    R_CONTA_CORRENTE "VARCHAR(15)",'
          '    R_AGENCIA "VARCHAR(10)",'
          '    R_COD_BANCO "CHAR(3)",'
          '    R_ATIVO "INTEGER",'
          '    R_RG_NUM "VARCHAR(11)",'
          '    R_RG_ORGAO_EMISSOR "VARCHAR(11)",'
          '    R_RG_UF "CHAR(2)",'
          '    R_CPF "VARCHAR(11)",'
          '    R_CPF_FTDO "VARCHAR(14)",'
          '    R_PIS_PASEP "CHAR(14)",'
          '    R_PIS_PASEP_FTDO "VARCHAR(18)")'
          'as'
          'declare variable VF_ID_PESSOA_FISICA "INTEGER";'
          'declare variable V_RET_SERVIDOR "CHAR(1)";'
          'declare variable VF_INICIA_SERVIDOR "CHAR(1)";'
          'declare variable VF_CALC_SAL "CHAR(1)";'
          'begin'
          ''
          
            '  for select D.ID_SUB_UNID_ORCAMENT, D.DESCR_SUB_UNID_ORCAMENT, ' +
            'D.ID, D.ID_PESSOA_FISICA, D.NOME_SERVIDOR,'
          
            '             D.ID_CARGO_ORIGEM, D.DESCR_CARGO_ORIGEM, D.ID_CARGO' +
            '_ATUAL, D.DESCR_CARGO_ATUAL, D.DT_ADMISSAO,'
          
            '             D.DT_READMISSAO, D.REF_SALARIO, D.VENCTO_BASE, D.TI' +
            'PO_SAL, D.ID_UNID_LOTACAO, D.DESCR_UNID_LOTACAO,'
          
            '             D.ID_EST_FUNCIONAL, D.DESCR_EST_FUNCIONAL, D.DESCR_' +
            'SITUAC_TCM, D.EFETIVO, D.ID_SITUACAO_TCM,'
          '             D.DESCR_EFETIVO, D1.INICIA_SERVIDOR, D1.CALC_SAL'
          '      from SERVIDOR D, ESTADO_FUNCIONAL D1'
          '      where (D.ID_UNID_GESTORA = :PE_UNID_GESTORA) and'
          '            (D1.ID = D.ID_EST_FUNCIONAL) and'
          '            (D1.INICIA_SERVIDOR = '#39'S'#39') and'
          
            '            (position('#39';'#39' || trim(cast(D.ID_SUB_UNID_ORCAMENT as' +
            ' char(120))) || '#39';'#39' in :PE_SUB_UNID_ORCAMS) > 0) and'
          
            '            ((position('#39';'#39' || trim(cast(D.ID_UNID_LOTACAO as cha' +
            'r(120))) || '#39';'#39' in :PE_UNID_LOTACOES) > 0) or (trim(cast(:PE_UNI' +
            'D_LOTACOES '
          'as varchar(120))) = '#39'0'#39')) and'
          
            '            ((position('#39';'#39' || trim(cast(D.ID_CARGO_ATUAL as char' +
            '(120))) || '#39';'#39' in :PE_CARGOS) > 0) or (trim(cast(:PE_CARGOS as v' +
            'archar(120))) '
          '= '#39'0'#39')) and'
          
            '            ((position('#39';'#39' || trim(cast(D.ID_EST_FUNCIONAL as ch' +
            'ar(80))) || '#39';'#39' in :PE_EST_FUNCIONAIS) > 0) or (trim(cast'
          '(:PE_EST_FUNCIONAIS as varchar(80))) = '#39'0'#39'))'
          
            '      into :R_ID_SUB_UNID_ORCAMENT, :R_DESCR_SUB_UNID_ORCAMENT, ' +
            ':R_ID_SERVIDOR, :R_ID_PESSOA_FISICA, '
          ':R_NOME_SERVIDOR,'
          
